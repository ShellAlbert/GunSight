prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/aarch64-linux-gnu
includedir=${prefix}/include

gmodule_supported=true

Name: GModule
Description: Dynamic module loader for GLib
Requires: glib-2.0
Version: 2.56.3
Libs: -L${libdir} -Wl,--export-dynamic -lgmodule-2.0 -pthread
Libs.private: -ldl
Cflags: -pthread
